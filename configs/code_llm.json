    "dataloader_drop_last": true,
    "deepspeed": "deepspeed_configs/stage_0.json",
    "torch_compile": true,
    "num_epochs": 1,
    "learning_rate": 2e-4,
    "weight_decay": 0.1,
    "eval_steps": 1000,
    "save_steps": 2000,
    "logging_steps": 20,
    "warmup_ratio": 0.02,
    "lr_scheduler_type": "cosine",
    "save_total_limit": 10,
    "seed": 42,
    "bf16": true,
    "bf16_full_eval": true,
    "output_dir": "ignore",
