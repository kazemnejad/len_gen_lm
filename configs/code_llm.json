{
    "dataset_name": "santacoder_data_tokenized_1024",
    "tokenizer_name": "santacoder_tokenizer",
    "evaluation_strategy": "steps",
    "save_strategy": "steps",
    "logging_strategy": "steps",
    "block_size": 1024,
    "preprocessing_num_workers": 128,
    "per_device_train_batch_size": 4,
    "gradient_accumulation_steps": 8,
    "per_device_eval_batch_size": 32,
    "dataloader_pin_memory": true,
    "dataloader_num_workers": 128,
    "dataloader_drop_last": true,
    "torch_compile": true,
    "lr_scheduler_type": "cosine",
    "learning_rate": 2e-4,
    "weight_decay": 0.1,
    "warmup_ratio": 0.02,
    "adam_beta1": 0.9,
    "adam_beta2": 0.95,
    "adam_epsilon": 1e-8,
    "num_train_epochs": 1,
    "eval_steps": 1000,
    "save_steps": 1000,
    "logging_steps": 20,
    "save_total_limit": 10,
    "report_to": "wandb",
    "seed": 42,
    "bf16": true,
    "bf16_full_eval": true,
    "output_dir": "ignore"
