{
    "dataset_name": "santacoder_data_tokenized_1024",
    "tokenizer_name": "santacoder_tokenizer",
    "evaluation_strategy": "steps",
    "save_strategy": "steps",
    "logging_strategy": "steps",
    "block_size": 1024,
    "preprocessing_num_workers": 128,
    "per_device_train_batch_size": 32,
